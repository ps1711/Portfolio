{
  "version": 2,
  "builds": [
    {
      "src": "mywebsite/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mywebsite/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles_build"
}
